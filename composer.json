{
    "name": "toriomlab/eloquent-form-elements",
    "description": "Simple eloquent model form elements generator package from Laravel",
    "keywords": ["Laravel","Form Generator", "Eloquent Form Generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Toriom Lab",
            "email": "info@toriom.com",
            "homepage": "http://toriom.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "ToriomLab\\EloquentFormElements\\": "src/"
        },
        "files": ["src/helpers.php"]
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
    "branch-alias": {
            "1.4.0": "1.4.x-dev",
            "1.5.0": "1.5.x-dev",
            "1.6.0": "1.6.x-dev",
            "1.7.0": "1.7.x-dev",
            "1.8.0": "1.8.x-dev",
        }
    }
}
